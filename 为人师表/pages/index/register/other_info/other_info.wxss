/* pages/index/register/other_info/other_info.wxss */
page{
	background: #eeeeee;
}
.container{
	padding: 0;
}
form {
	width: 100%;
}
.user-icon {
	width: 100%;
	height: 300rpx;
	display: flex;
	justify-content: center;
	align-items: center;
}
.user-icon image {
	width: 250rpx;
	height: 250rpx;
}
.list {
	width: 100%;
	background: #ffffff;
	display: flex;
	flex-direction: column;
	align-items: center;
}
.list .setting {
	width: 710rpx;
	height: 80rpx;
	border-bottom: solid #eeeeee 1rpx;
	display: flex;
	flex-direction: row;
	justify-content: space-between;
	align-items: center;
}
.list .setting .left {
	padding-left: 20rpx;
	width:200rpx;
}
.list .setting .right {
	padding-right: 20rpx;
	color: #999999;
	width: 510rpx;
	text-align: right;
}
.button {
	position: absolute;
	bottom: 20rpx;
}
.button button {
	margin: 20rpx;
	width: 710rpx;
	background: #3f99f8;
	color: #ffffff;
}